  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>WITH games AS (
    SELECT
        hc.club_id, hc.club_code, 'Home' AS venue,
        COUNT(*) AS games_cnt,
        SUM(CASE WHEN f.home_club_goals &gt; f.away_club_goals THEN 1 ELSE 0 END) AS wins,
        SUM(CASE WHEN f.home_club_goals = f.away_club_goals THEN 1 ELSE 0 END) AS draws,
        SUM(CASE WHEN f.home_club_goals &lt; f.away_club_goals THEN 1 ELSE 0 END) AS losses,
        SUM(f.home_club_goals) AS goals_for,
        SUM(f.away_club_goals) AS goals_against
    FROM ProjectDw.FactGame f
    JOIN ProjectDw.DimClub hc ON hc.club_id = f.home_club_id
    GROUP BY hc.club_id, hc.club_code

    UNION ALL

    SELECT
        ac.club_id, ac.club_code, 'Away' AS venue,
        COUNT(*) AS games_cnt,
        SUM(CASE WHEN f.away_club_goals &gt; f.home_club_goals THEN 1 ELSE 0 END) AS wins,
        SUM(CASE WHEN f.away_club_goals = f.home_club_goals THEN 1 ELSE 0 END) AS draws,
        SUM(CASE WHEN f.away_club_goals &lt; f.home_club_goals THEN 1 ELSE 0 END) AS losses,
        SUM(f.away_club_goals) AS goals_for,
        SUM(f.home_club_goals) AS goals_against
    FROM ProjectDw.FactGame f
    JOIN ProjectDw.DimClub ac ON ac.club_id = f.away_club_id
    GROUP BY ac.club_id, ac.club_code
)
SELECT
    club_id, club_code, venue, games_cnt,
    wins, draws, losses,
    CAST((wins*3.0 + draws*1.0)/NULLIF(games_cnt,0) AS DECIMAL(10,2)) AS ppg,
    (goals_for - goals_against) AS goal_diff,
    CAST(wins*1.0/NULLIF(games_cnt,0) AS DECIMAL(10,4)) AS win_rate
FROM games
ORDER BY club_code, venue;
</CommandText>
      </Query>
