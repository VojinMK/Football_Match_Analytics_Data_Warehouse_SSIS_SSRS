  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
    c.competition_name,
	IIF(c.competition_country_name='Nepoznato','Europe Tournament',c.competition_country_name),
    s.season_name,
    AVG(CAST(f.home_club_goals + f.away_club_goals AS DECIMAL(10,2))) AS avg_goals,
    COUNT(*) AS matches_count
FROM ProjectDW.FactGame f
JOIN ProjectDW.DimCompetition c ON c.competition_id = f.competition_id
JOIN ProjectDW.DimSeason     s ON s.season_id      = f.season_id
GROUP BY c.competition_name,c.competition_country_name, s.season_name
ORDER BY c.competition_name, s.season_name;</CommandText>
      </Query>
      <Fields>
        <Field Name="competition_name">
          <DataField>competition_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="competition_country">
