  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
    s.season_name,
    st.stadium_id,
    st.stadium_name,
    COUNT(*) AS matches_cnt,
    AVG(CAST(f.attendance AS DECIMAL(12,2))) AS avg_attendance
FROM ProjectDw.FactGame f
JOIN ProjectDw.DimStadium st ON st.stadium_id = f.stadium_id
JOIN ProjectDw.DimSeason  s  ON s.season_id   = f.season_id
GROUP BY
    s.season_name,
    st.stadium_id,
    st.stadium_name
HAVING COUNT(*) &gt;= 5 and  AVG(CAST(f.attendance AS DECIMAL(12,2)))&gt;0
ORDER BY
    s.season_name,
    avg_attendance DESC,
    matches_cnt DESC,
    st.stadium_name;
</CommandText>
      </Query>
      <Fields>
        <Field Name="season_name">
          <DataField>season_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="stadium_name">
          <DataField>stadium_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="stadium_id">
          <DataField>stadium_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="matches_cnt">
          <DataField>matches_cnt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="avg_attendance">
          <DataField>avg_attendance</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="avg_attendace_correct">
          <Value>=CDbl(Replace(Fields!avg_attendance.Value, ",", "."))</Value>
        </Field>
      </Fields>
    </DataSet>
