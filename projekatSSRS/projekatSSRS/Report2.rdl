  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>WITH base AS (
    SELECT
        r.referee_name,
        COUNT(*) AS matches_cnt,
        SUM(f.home_yellow_cards + f.away_yellow_cards) AS yellow_sum,
        SUM(f.home_red_cards    + f.away_red_cards)    AS red_sum
    FROM ProjectDW.FactGame f
    JOIN ProjectDW.DimReferee r ON r.referee_id = f.referee_id
    GROUP BY r.referee_id, r.referee_name
)
SELECT
    referee_name,
    matches_cnt,
    yellow_sum AS total_yellow_cards,
    red_sum    AS total_red_cards,
    CAST(yellow_sum * 1.0 / NULLIF(matches_cnt, 0) AS DECIMAL(10,2)) AS avg_yellow_per_match,
    CAST(red_sum    * 1.0 / NULLIF(matches_cnt, 0) AS DECIMAL(10,2)) AS avg_red_per_match,
    CAST((yellow_sum + red_sum) * 1.0 / NULLIF(matches_cnt, 0) AS DECIMAL(10,2)) AS avg_total_cards
FROM base
</CommandText>
      </Query>
